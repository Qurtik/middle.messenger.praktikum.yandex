{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"baseUrl": ".",
		"paths": {
			"@/*": [
				"./src/*"
			]
		},
		"sourceMap": true,
		"noImplicitAny": false,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"strictNullChecks": false,
		"moduleResolution": "node",
	},
	"include": [
		"src/**/*.ts",
		".esliintrc.cjs"
	]
}
