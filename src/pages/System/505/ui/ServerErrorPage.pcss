.not-found-page{
	display: flex;
	justify-content: center;
	align-items: center;
	flex-direction: column;
	width: 100%;
	height: 100%;
}

.not-found-page__main{
	display: flex;
	justify-content: center;
	align-items: center;
	flex-direction: column;
}

.not-found-page__main-title{
	font-size: 85px;
	font-family: Arial, Helvetica, sans-serif;
}

.not-found-page__main-message{
	font-size: 25px;
	font-family: Arial, Helvetica, sans-serif;
}

.not-found-page__link{
	margin-top: 85px;
}
